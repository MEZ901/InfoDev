{
  "name": "infodev",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "node .",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:tailwindcss\" \"npm run dev:ejs\"",
    "dev:server": "nodemon .",
    "dev:tailwindcss": "npx tailwindcss -i ./public/css/main.css -o ./public/css/output.css --watch",
    "dev:ejs": "browser-sync start --proxy 'localhost:3000' --files './src/views/**/*.ejs'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.3.1",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "browser-sync": "^2.29.3",
    "concurrently": "^8.2.1",
    "nodemon": "^3.0.1",
    "prisma": "^5.3.1",
    "tailwindcss": "^3.3.3"
  }
}
